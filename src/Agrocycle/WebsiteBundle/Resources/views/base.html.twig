<!DOCTYPE html>
<!--[if IE 8]> <html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->

<head>
   
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Agrocycle</title>
    
    <link rel="stylesheet" type="text/css"  href="{{ asset('bundles/agrocyclewebite/css/normalize.css') }}">
    
    <link rel="stylesheet" type="text/css"  href="{{ asset('bundles/agrocyclewebsite/css/foundation.css') }}">
    
    <link rel="stylesheet" type="text/css"  href="{{ asset('bundles/agrocyclewebsite/css/app.css') }}">

    <script src="{{ asset('bundles/agrocyclewebsite/js/vendor/custom.modernizr.js') }}"></script>

</head>
<body>

    <div class="container">
    
        <nav class="main">

            <div class="row">

                  <div class="large-12 columns text-right secondary">
                      <a href="#">Nieuwsbrief</a> <a href="#">Contacteer Ons</a>
                  </div>

            </div>



            <div class="row">


                <div class="large-4 columns">
                    <div class="logo">
                        <img src="{{asset('bundles/agrocyclewebsite/img/agrocycle/logo_agrocycle.png')}}" alt="Agrocycle">
                    </div>
                </div>

                <div class="large-8 columns text-right">
                      <ul class="primary">
                        <li {% if (app.request.attributes.get('_route') == 'agrocycle_website_default_index') %} class="active" {% endif %}><a href="{{path('agrocycle_website_default_index')}}">Home</a></li>
                        <li {% if (app.request.attributes.get('_route') == 'agrocycle_website_default_pioniers') %} class="active" {% endif %}><a href="{{path('agrocycle_website_default_pioniers')}}">Pioniers</a></li>
                        <li {% if (app.request.attributes.get('_route') == 'agrocycle_website_default_sectoren') %} class="active" {% endif %}><a href="#">Sectoren</a></li>
                        <li {% if (app.request.attributes.get('_route') == 'agrocycle_website_default_results') %} class="active" {% endif %}><a href="#">Hefbomen&amp;Barrières</a></li>
                      </ul>
                </div>

            </div>

        </nav>

        <section class="row">
            <header class="large-12 columns tagline">
               {% block tagline %}
               {% endblock %}
            </header>
        </section>
      
    </div>
    
    <section class="content">
        
        <div class="main-content">
            
            {%block body%}
            {% endblock %}
            
        </div>
        
    </section>
    
    <footer>
        
        <div class="row">
        
            <div class="large-8 columns">
                <ul class="inline-list">
                    <li><a href="#">Home</a></li>
                    <li><a href="#">Pioniers</a></li>
                    <li><a href="#">Sectoren</a></li>
                    <li><a href="#">Hefbomen&amp;Barrières</a></li>
                    <li><a href="#">Contacteer ons</a></li>
                </ul>
            </div>
            
            <div class="large-4 columns text-right">
                <p>&copy; 2013 Agrocycle</p>
            </div>
            
        </div>
        
        <section class="sponsors">
        
        <div class="row">
            
            
            
                <div class="large-4 columns text-center">

                    <a href="#"><img src="{{asset('bundles/agrocyclewebsite/img/agrocycle/boerenbond.png')}}"></a>

                </div>

                <div class="large-4 columns text-center">

                    <a href="#"><img src="{{asset('bundles/agrocyclewebsite/img/agrocycle/vlaams-brabant.png')}}"></a>

                </div>

                <div class="large-4 columns text-center">

                    <a href="#"><img src="{{asset('bundles/agrocyclewebsite/img/agrocycle/khleuven.png')}}"></a>

                </div>

          
            
        </div>
            
              </section>
        
 
    </footer>
    

	

    <script>
    document.write('<script src=' +
    ('__proto__' in {} ? '/web/bundles/agrocyclewebsite/js/vendor/zepto' : '/web/bundles/agrocyclewebsite/js/vendor/jquery') +
    '.js><\/script>')
    </script>

    <script src="{{ asset('bundles/agrocyclewebsite/js/foundation.min.js') }}"></script>


    <script>
      $(document).foundation();
    </script>
    
    {% block customjs %}
   
    {% endblock %}
</body>
</html>
